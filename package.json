{
  "name": "restaurant-voice-agent",
  "version": "1.0.0",
  "description": "Restaurant booking voice agent using Ultravox and Twilio",
  "main": "dist/src/server.enhanced.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.enhanced.ts",
    "build": "tsc",
    "start": "node dist/src/server.enhanced.js",
    "test": "NODE_ENV=test jest --env-file=.env.test",
    "test:watch": "NODE_ENV=test jest --watch --env-file=.env.test",
    "test:coverage": "NODE_ENV=test jest --coverage --env-file=.env.test",
    "test:reset": "NODE_ENV=test ./scripts/reset-test-db.sh",
    "test:clear": "NODE_ENV=test ./scripts/clear-test-data.sh",
    "lint": "eslint *.ts",
    "format": "prettier --write *.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "chalk": "^4.1.2",
    "chrono-node": "^2.8.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "node-fetch": "^2.7.0",
    "twilio": "^4.23.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.1",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "restaurant",
    "voice-agent",
    "ultravox",
    "twilio",
    "booking",
    "typescript",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT"
}
